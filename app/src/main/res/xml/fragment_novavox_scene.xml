<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        android:id="@id/mainImageView"
        motion:duration="2000"
        motion:autoTransition="animateToEnd"
        motion:motionInterpolator="linear"
        motion:constraintSetEnd="@id/end"
        motion:constraintSetStart="@id/start">
        <KeyFrameSet>
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/vaccine"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.45"
            motion:layout_constraintHorizontal_bias="0.63"
            android:alpha="0" />
        <Constraint
            android:id="@+id/vizor"
            motion:layout_constraintStart_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.37" />
        <Constraint
            android:id="@+id/mainImageView"
            android:layout_width="380dp"
            android:layout_height="match_parent"
            motion:layout_constraintEnd_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/textView"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="@id/linearTow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@id/linearTow"
            motion:layout_constraintBottom_toBottomOf="@id/linearTow"
            motion:layout_constraintVertical_bias="0.0"
            android:alpha="0" />
        <Constraint
            android:id="@+id/View1"
            android:elevation="1dp"
            motion:layout_constraintEnd_toEndOf="@id/linearTow"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/linearTow"
            motion:layout_constraintStart_toStartOf="@id/linearTow"
            android:layout_marginBottom="8dp" />
        <Constraint
            android:id="@+id/lottiLayout"
            motion:layout_constraintEnd_toEndOf="@id/View1"
            android:layout_width="0dp"
            android:layout_height="125dp"
            motion:layout_constraintBottom_toBottomOf="@id/View1"
            motion:layout_constraintStart_toStartOf="@id/View1" />
        <Constraint
            android:id="@+id/textVieweffic"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="@id/linearTow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@id/linearTow"
            motion:layout_constraintStart_toStartOf="@id/linearTow"
            android:layout_marginTop="18dp"
            motion:layout_constraintBottom_toBottomOf="@id/textView"
            motion:layout_constraintVertical_bias="0.10" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/vaccine"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.45"
            motion:layout_constraintHorizontal_bias="0.63"
            android:alpha="1" />
        <Constraint
            android:id="@+id/vizor"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.37"
            motion:layout_constraintHorizontal_bias="0.3" />
        <Constraint
            android:id="@+id/mainImageView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/textView"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="@id/linearTow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@id/linearTow"
            motion:layout_constraintStart_toStartOf="@id/linearTow"
            motion:layout_constraintBottom_toBottomOf="@id/linearTow"
            motion:layout_constraintVertical_bias="0.15"
            android:alpha="1"
            android:layout_marginTop="16dp" />
        <Constraint
            android:id="@+id/View1"
            android:elevation="1dp"
            motion:layout_constraintEnd_toEndOf="@id/linearTow"
            android:layout_width="0dp"
            android:layout_height="240dp"
            motion:layout_constraintBottom_toBottomOf="@id/linearTow"
            motion:layout_constraintStart_toStartOf="@id/linearTow" />
        <Constraint
            android:id="@+id/lottiLayout"
            motion:layout_constraintEnd_toEndOf="@id/View1"
            android:layout_width="0dp"
            android:layout_height="380dp"
            motion:layout_constraintBottom_toBottomOf="@id/View1"
            motion:layout_constraintStart_toStartOf="@id/View1" />
        <Constraint
            android:id="@+id/textVieweffic"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="@id/linearTow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@id/linearTow"
            motion:layout_constraintStart_toStartOf="@id/linearTow"
            android:layout_marginTop="18dp"
            motion:layout_constraintBottom_toBottomOf="@id/textView"
            motion:layout_constraintVertical_bias="0.10" />
    </ConstraintSet>
</MotionScene>
